{
  "name": "code-i18n",
  "version": "2.5.1",
  "description": "Intrusive language conversion script",
  "main": "index.js",
  "types": "types/index.d.ts",
  "bin": "bin.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "t": "tsc src/**/*.ts --declaration --downlevelIteration --emitDeclarationOnly --resolveJsonModule --esModuleInterop --moduleResolution node --target es2020 --outDir types"
  },
  "keywords": [
    "i8n"
  ],
  "author": "Link",
  "license": "BSD",
  "files": [
    "__tests__",
    "src",
    "types"
  ],
  "bugs": {
    "url": "https://github.com/Linkontoask/code-i18n/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Linkontoask/code-i18n.git"
  },
  "dependencies": {
    "@babel/generator": "^7.14.5",
    "@babel/parser": "^7.14.7",
    "@babel/runtime": "^7.14.7",
    "@babel/traverse": "^7.14.7",
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "glob": "^7.1.7",
    "inquirer": "^8.1.2",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "prettier": "^2.3.2",
    "vue-eslint-parser-private": "7.9.0-private"
