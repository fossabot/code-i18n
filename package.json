    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.171",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/parser": "^4.29.1",
    "jest": "^27.0.6",
    "recast": "^0.20.5",
