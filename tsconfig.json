{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "moduleResolution": "Node"
  },
  "include": ["src/**/*"]
}
